{
  "cli": {
    "version": ">= 18.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_KEY": "YOUR_VALUE",
        "EXPO_PUBLIC_AUTH_DOMAIN": "chroma.firebaseapp.com",
        "EXPO_PUBLIC_PROJECT_ID": "YOUR_VALUE",
        "EXPO_PUBLIC_STORAGE_BUCKET": "YOUR_VALUE",
        "EXPO_PUBLIC_MESSAGING_SENDER_ID": "YOUR_VALUE",
        "EXPO_PUBLIC_APP_ID": "YOUR_VALUE",
        "EXPO_PUBLIC_MEASUREMENT_ID": "YOUR_VALUE",
        "EXPO_PUBLIC_GEMINI_API_KEY": "YOUR_VALUE"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
